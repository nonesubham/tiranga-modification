                                    <span data-v-3966082f="" style="color:#B1835A;">₹ ${data.money}.00</span>
                                </div>
                            </div>
                            <div data-v-3966082f="" class="time">${timerJoin(data.time)}</div>
                        </div>
                        <div data-v-3966082f="">
                            <div data-v-3966082f="" class="state m-b-5">
                                <!---->
                                <div data-v-4e585e69="" style="color: ${(data.status == 1) ? 'rgb(107, 190, 88)' : (data.status == 0) ? 'rgb(255 171 0);' : 'red'}"> ${(data.status == 1) ? 'Success' : (data.status == 2) ? 'Cancelled' : 'Pending'}</div>
                                <!---->
                            </div>
                        </div>
                    </div>
                    `;
            })
            $('.van-list .lisys').html(html);
            $('.number').click(function (e) {
                e.preventDefault();
                let text = $(this).text().trim();
                navigator.clipboard.writeText(text);
                $(this).addClass('block-click');
                alertMess('Copy successful', $(this));
            });
        }
        function alertMess(text, sic) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, 1000);
        }
        $.ajax({
            type: "GET",
            url: "/api/webapi/recharge/list",
            dataType: "json",
            success: function (response) {
                if (response.datas.length == 0) return;
                loads(response.datas);
            }
        });

        const searchParams = new URLSearchParams(window.location.search);

        const clientTxnId = searchParams.get("client_txn_id")
        const txnId = searchParams.get("txn_id")

        if (!!txnId && !!clientTxnId) {
            $.ajax({
                type: "POST",
                url: "/api/webapi/confirm_recharge",
                data: {
                    client_txn_id: clientTxnId,
                    txn_id: txnId,
                },
                dataType: "json",
                success: function (response) {
                    window.location.href = `${window.location.origin}${window.location.pathname}`
                    //window.location.reload()
                    localStorage.removeItem("userData")
                }
            });
        }
    </script>
</body>

</html>