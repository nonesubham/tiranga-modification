                            <script>
                                document.addEventListener('DOMContentLoaded', function() {
                                    const tabs = document.querySelectorAll('.tab_item');
                                    const activeLine = document.querySelector('.fun-tabs__active-line');
                                    const tabList = document.querySelector('.fun-tabs__tab-list');

                                    function updateActiveLine(tab) {
                                        const tabRect = tab.getBoundingClientRect();
                                        const tabListRect = tabList.getBoundingClientRect();
                                        const tabListLeft = tabListRect.left;

                                        // Calculate the width and position of the active line
                                        activeLine.style.width = `${tabRect.offsetWidth}px`;
                                        activeLine.style.transform = `translate3d(${tabRect.left - tabListLeft}px, 0px, 0px)`;
                                    }

                                    function scrollToTab(tab) {
                                        const tabRect = tab.getBoundingClientRect();
                                        const tabListRect = tabList.getBoundingClientRect();

                                        // Calculate the scroll position to bring the tab into view
                                        const scrollLeft = tabRect.left - tabListRect.left + tabList.scrollLeft - (tabListRect.width / 2) + (tabRect.width / 2);
                                        tabList.scrollTo({
                                            left: scrollLeft,
                                            behavior: 'smooth'// Smooth scroll animation
                                        });
                                    }

                                    tabs.forEach(tab=>{
                                        tab.addEventListener('click', function() {
                                            // Remove 'tab_active' class from all tabs
                                            tabs.forEach(t=>t.classList.remove('tab_active'));

                                            // Add 'tab_active' class to the clicked tab
                                            this.classList.add('tab_active');

                                            // Update the active line position and width
                                            updateActiveLine(this);

                                            // Scroll to the clicked tab
                                            scrollToTab(this);
                                        });
                                    }
                                    );

                                    // Set initial active line position
                                    const initialTab = document.querySelector('.tab_active');
                                    if (initialTab) {
                                        updateActiveLine(initialTab);
                                        scrollToTab(initialTab);
                                    }
                                });
