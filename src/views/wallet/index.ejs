            selectBoxes.forEach(otherBox => {
                if (otherBox !== box) {
                    otherBox.classList.remove('selected');
                }
            });

            
            box.classList.toggle('selected');

            
            const url = box.dataset.url;
            if (url) {
                window.location.href = url; 
            }
        });
    });
});

